//查看都占用
sudo lsof -i -P -n

443改8443
sed -i 's/Listen 443/Listen 8443/' /etc/apache2/ports.conf
sed -i 's/:443/:8443/' /etc/apache2/sites-available/default-ssl.conf
a2ensite *
apache2ctl configtest
systemctl reload apache2
systemctl restart apache2
systemctl status apache2

443改回8443
sed -i 's/Listen 8443/Listen 443/' /etc/apache2/ports.conf
sed -i 's/:8443/:443/' /etc/apache2/sites-available/default-ssl.conf
a2ensite *
apache2ctl configtest
systemctl reload apache2
systemctl restart apache2
systemctl status apache2
